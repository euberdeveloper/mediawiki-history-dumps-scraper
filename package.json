{
  "name": "mhdscraper",
  "version": "1.0.1",
  "description": "Mediawiki history dumps scraper, a module that scrapes the site and returns to you the available content.",
  "main": "bundled/index.js",
  "types": "bundled/index.d.ts",
  "scripts": {
    "clean": "shx rm -rf dist bundled",
    "transpile": "tsc -p source",
    "pretranspile": "npm run clean",
    "prebundle": "npm run clean",
    "bundle": "webpack",
    "lint": "eslint source --ext ts --format codeframe",
    "lint:fix": "eslint source --ext ts --format codeframe --fix",
    "docs:html": "typedoc",
    "docs:html-dev": "typedoc --options typedoc.dev.js",
    "docs": "npm run docs:html && npm run docs:html-dev",
    "commit": "git-cz",
    "commit:sign": "git-cz -S",
    "prepublishOnly": "npm run bundle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/euberdeveloper/mediawiki-history-dumps-scraper.git#node"
  },
  "files": [
    "package.json",
    "bundled",
    "README.md",
    "LICENSE"
  ],
  "author": "Eugenio Berretta <euberdeveloper@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@euberdeveloper/eslint-plugin": "^1.1.3",
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.2.0",
    "dts-bundle-webpack": "^1.0.2",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-unicorn": "^34.0.1",
    "prettier": "^2.3.2",
    "shx": "^0.3.3",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3",
    "webpack": "^5.27.0",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "axios": "^0.21.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
