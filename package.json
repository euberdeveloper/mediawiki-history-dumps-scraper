{
  "name": "whdtscraper-server",
  "version": "1.0.0",
  "description": "Wikimedia history dump tsv scraper, a module that scrapes the site and returns to you the available content.",
  "main": "bundled/index.js",
  "types": "bundled/index.d.ts",
  "scripts": {
    "clean": "shx rm -rf dist bundled",
    "transpile": "tsc -p source",
    "pretranspile": "npm run clean",
    "prebundle": "npm run clean",
    "bundle": "webpack",
    "lint": "eslint source --ext ts --format codeframe",
    "lint:fix": "eslint source --ext ts --format codeframe --fix",
    "docs:html": "typedoc",
    "docs:html-dev": "typedoc --options typedoc.dev.js",
    "docs": "npm run docs:html && npm run docs:html-dev",
    "commit": "git-cz",
    "commit:sign": "git-cz -S",
    "prepublishOnly": "npm run bundle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/euberdeveloper/wikimedia-history-dump-scraper.git#node"
  },
  "files": [
    "package.json",
    "bundled",
    "README.md",
    "LICENSE"
  ],
  "author": "Eugenio Berretta <euberdeveloper@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@euberdeveloper/eslint-config-typescript": "^4.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.35",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "shx": "^0.3.3",
    "typescript": "^4.2.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "compress": "^0.99.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "morgan": "^1.10.0",
    "whdtscraper": "^1.0.0"
  }
}
